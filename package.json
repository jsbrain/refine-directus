{
  "name": "@workatease/refine-directus",
  "description": "Refine Directus REST data provider. This is a wrapper around the Directus SDK to use in refine.",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "git@github.com:workatease/refine-directus.git",
  "private": false,
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch --tsconfig tsconfig.json --verbose --noClean",
    "build": "tsdx build --tsconfig tsconfig.json",
    "test": "tsdx test --passWithNoTests --runInBand",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "author": "Pyll Gomez",
  "module": "dist/refine-directus.esm.js",
  "size-limit": [
    {
      "path": "dist/refine-directus.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/refine-directus.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.5",
    "nock": "^13.1.3",
    "size-limit": "^5.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1"
  },
  "dependencies": {
    "@directus/sdk": "^9.8.0",
    "@pankod/refine-core": "^3.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}